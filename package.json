{
    "name": "shamela-tts-reader",
    "version": "1.0.0",
    "description": "Full-stack web application for blind users to access Maktaba Shamela books with TTS playback",
    "private": true,
    "type": "module",
    "scripts": {
        "install:all": "cd backend && npm install && cd ../frontend && npm install",
        "dev:backend": "cd backend && npm run dev",
        "dev:frontend": "cd frontend && npm run dev",
        "migrate": "cd backend && npm run migrate",
        "seed": "cd backend && npm run seed",
        "import:book": "powershell -ExecutionPolicy Bypass -File scripts/import_book.ps1"
    },
    "keywords": [
        "shamela",
        "tts",
        "accessibility",
        "books",
        "arabic"
    ],
    "author": "",
    "license": "MIT"
}